{
  "name": "@zunaib.imtiaz/react-npm-package",
  "version": "1.1.1",
  "description": "A Nice Test NPM Package",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zunaib/react-npm-package.git"
  },
  "keywords": [
    "Test",
    "Npm",
    "Package"
  ],
  "author": "Zunaib Imtiaz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Zunaib/react-npm-package/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/Zunaib/react-npm-package#readme",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
